{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
     "forceConsistentCasingInFileNames": true, // 强制文件名大小写一致
    "strict": true // 启用严格类型检查
  },
  "include": ["vite.config.ts"]
}